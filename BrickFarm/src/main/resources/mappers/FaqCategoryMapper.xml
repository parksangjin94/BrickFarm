	<!-- ==[염세환]========================================================================================================================================== -->
	<!-- ================================================================================================================================================== -->

	<!-- ==[박상진]========================================================================================================================================== -->
	<!-- ================================================================================================================================================== -->

	<!-- ==[송영태]========================================================================================================================================== -->
	<!-- ================================================================================================================================================== -->

	<!-- ==[김용진]========================================================================================================================================== -->
	<select id="selectAllFaqCategory" resultType="UserFaqCategory">
		SELECT * FROM faq_category
		ORDER BY faq_category_no
	</select>

	<insert id="insertFaqCategory">
		INSERT INTO faq_category(faq_category_name)
		VALUES(#{faqCategoryName})
	</insert>

	<update id="updateFaqCategoryByCategoryNo">
		UPDATE faq_category
		SET faq_category_name = #{faq_category_name}
		WHERE faq_category_no = #{faq_category_no}
	</update>

	<delete id="deleteFaqCategoryByCategoryNo">
		DELETE FROM faq_category
		WHERE faq_category_no = #{faqCategoryNo}
	</delete>
	<!-- ================================================================================================================================================== -->
</mapper>